{
  "name": "bettingon_dapp",
  "version": "0.0.1",
  "description": "Bettingon contracts dapp in react/redux",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --port 9090 --debug --hot --open --content-base build",
    "deploy": "eslint -c .eslintrc.json dapp --quiet && npm run build && npm run deploy-dapp",
    "serve": "npm run build && http-server-spa build",
    "testrpc": "testrpc -d -u 0 -u 1 -u 9 -l 4700000000000 ",
    "contract-update": "truffle migrate --reset",
    "dapprpc": "run-p testrpc eth-bridge",
    "devnode-up": "run-p dapprpc dapp-dev",
    "delay": "sleep 50s",
    "dapp-dev": "run-s delay contract-update",
    "eth-bridge": "cd node_modules/ethereum-bridge && node bridge.js -a 9 -H localhost:8545 --gas 4700000 --dev ",
    "eslint": "eslint -c dapp/.eslintrc.json dapp"
  },
  "author": "Juan Carlos Escamilla",
  "license": "GPL-3.0",
  "dependencies": {
    "bettingon": "git+ssh://git@github.com/blocktisans/bettingon.git",
    "bip39": "^2.4.0",
    "enzyme": "^2.9.1",
    "ethereumjs-wallet": "^0.6.0",
    "ethpm": "0.0.14",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-duration-format": "^1.3.0",
    "node-fetch": "^1.7.3",
    "node-fetch-json": "0.0.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-blockies": "^1.1.6",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-page-layout": "^0.9.6",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-table": "^6.5.1",
    "react-web3": "^0.4.3",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.71.3",
    "truffle-contract": "^1.1.11",
    "web3": "^0.18.4",
    "web3-provider-engine": "^13.2.9"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.4",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^2.3.1",
    "ethereum-bridge": "git+https://github.com/jcxk/ethereum-bridge.git",
    "ethereumjs-testrpc": "^4.1.3",
    "html-webpack-plugin": "^2.29.0",
    "http-server-spa": "^1.2.0",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.13.2",
    "surge": "^0.19.0",
    "truffle": "*",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-hot-middleware": "^2.18.2"
  }
}
